   "source": [
    "def multipleOf(num1, num2, range1, range2):\n",
    "    n3 = num1*num2\n",
    "    # for first divisible num\n",
    "    for i in range(range1,range1 + n3 +1):\n",
    "        if i % n3 == 0:\n",
    "            f1 = i\n",
    "            break\n",
    "    def recursive(f1):\n",
    "        if f1 <= range2:\n",
    "            print(f1)\n",
    "            recursive(f1 + 35)\n",
    "    recursive(f1)\n",
    "\n",
    "n1,n2 = 7, 5\n",
    "r1, r2 = 1500,2700\n",
    "\n",
    "multipleOf(n1,n2,r1,r2)        "
   ]
